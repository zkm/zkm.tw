{
  "name": "zkm.tw",
  "private": true,
  "version": "4.0.4",
  "type": "module",
  "repository": "git@github.com:zkm/zkm.tw.git",
  "author": "Zach Schneider <me@zachschneider.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "deploy": "yarn build && git checkout production && rsync -avh --progress dist/ . && git add . && git commit -m 'Update production build for deployment' && git push origin production && git checkout master",
    "docker:dev": "docker-compose -f docker-compose.yml up",
    "docker:prod": "docker-compose -f docker-compose.yml up web-prod"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chart.js": "^4.5.0",
    "framer-motion": "^12.23.12",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.539.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-gtm-module": "^2.0.11",
    "styled-components": "^6.1.19",
    "styled-system": "^5.1.5",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/react-gtm-module": "^2.0.1",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vitest": "^3.2.4"
  },
  "browserslist": {
